{
    "name": "telkins/laravel-inquiry",
    "description": "A (somewhat opinionated) Laravel package to help make asking questions and getting answers a little easier and more uniform.",
    "keywords": [
        "telkins",
        "laravel",
        "inquiry",
        "question",
        "ask",
        "interrogate",
        "answer",
        "query",
        "laravel-inquiry"
    ],
    "homepage": "https://github.com/telkins/laravel-inquiry",
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Elkins",
            "email": "travis.elkins+github@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^7.0 || ^8.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.0|^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Telkins\\LaravelInquiry\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Telkins\\LaravelInquiry\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
